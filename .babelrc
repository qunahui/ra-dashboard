{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      },
      "react-hot-loader/babel"
    ],
    [
      "import", {
      "libraryName": "antd",
      "libraryDirectory": "es" //or "lib" for default
      //No "style" setting
    }
    ]
  ]
}
